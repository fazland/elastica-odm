{
    "name": "fazland/elastica-odm",
    "minimum-stability": "stable",
    "description": "",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fazland DevTeam",
            "email": "devs@fazland.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.6",
        "doctrine/common": "^2.8",
        "kcs/class-finder": "^0.1",
        "kcs/metadata": "^2.1|^3.0",
        "ocramius/proxy-manager": "^2.0",
        "psr/cache-implementation": "^1.0",
        "psr/log": "^1.0",
        "ruflin/elastica": "^5.0|^6.0",
        "symfony/console": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/cache": "^3.1|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\ODM\\Elastica\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\ODM\\Elastica\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "archive": {
        "exclude": ["tests", "vendor", ".php_cs", ".gitignore", "phpunit.xml.dist", "README.md"]
    }
}
